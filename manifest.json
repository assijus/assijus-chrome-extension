{
    "name": "Assijus Signer",
    "version": "1.3.1.0",
    "background": { "scripts": ["background.js"] },
    "permissions": ["nativeMessaging"],
    "externally_connectable": {
        "matches": ["https://assijus.trf2.jus.br/assijus/*", "https://assijus.jfrj.jus.br/assijus/*", "https://assijus.trf4.jus.br/*",
            "https://assijus.desenvolvimento.spsempapel.sp.gov.br/*", "https://assijus.homologacao.spsempapel.sp.gov.br/*", "https://assijus.spsempapel.sp.gov.br/*",
            "*://sigat.jfrj.jus.br/assijus/*", "https://sigah.jfrj.jus.br/assijus/*", "https://assijush.jfrj.jus.br/assijus/*",
            "*://homologa.assijus.mt.gov.br/assijus/*", "*://assijus.mt.gov.br/assijus/*",
            "*://assijus.processo.rio/assijus/*", "*://assijush.processo.rio/assijus/*",
            "*://assijus.telebras.com.br/*", "*://assijus-homologacao.telebras.com.br/*", "*://assijus-desenvolvimento.telebras.com.br/*",
            "*://assijus.infraero.gov.br/*", "*://assijushml.infraero.gov.br/*", "*://assijusnav.infraero.gov.br/*",
            "*://assijus.riopreto.sp.gov.br/*",
            "*://assijus.campinas.sp.leg.br/*",
            "*://assijus.finep.gov.br/*",
            "*://ittrufusion.appspot.com/*",
            "*://localhost/assijus/*", "*://localhost/*"
        ]
    },
    "browser_action": {
        "default_icon": {
            "16": "icon16.png",
            "32": "icon32.png",
            "96": "icon96.png"
        },
        "default_title": "Assijus Signer"
    },
    "icons": {
        "16": "icon16.png",
        "32": "icon32.png",
        "96": "icon96.png"
    },
    "manifest_version": 3
}